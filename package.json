{
  "name": "teachyourselfmath",
  "version": "0.1.0",
  "description": "make learning math fun!",
  "main": "src/index.js",
  "author": "viveknathani",
  "license": "MIT",
  "scripts": {
    "lint": "npx eslint --fix src/",
    "format": "npx prettier --write src/",
    "build": "yarn lint && yarn format && yarn tsc",
    "start": "yarn build && node build/index.js",
    "migrate": "node-pg-migrate -m src/database/migrations",
    "migrate:up": "yarn migrate up",
    "migrate:down": "yarn migrate down",
    "migrate:create": "yarn migrate create"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/pg": "^8.10.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.52.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-winston": "^4.2.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.11.3",
    "winston": "^3.11.0"
  }
}
